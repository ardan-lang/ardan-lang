print("=== Array & Object Edge Cases ===");

let a = [1,2,3];
let b = [a, [4,5], 6];
print(b[0][1], b[1][0]);

let o = {foo:123, bar:{baz:42}};
print(o.bar.baz);
o.bar.qux = [7,8];
print(o.bar.qux[1]);

a.push(44); print(a);
a.pop(); print(a);

